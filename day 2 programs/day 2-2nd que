y=float(input("enter a year:"))
if(y==1936):
   print("enter a valid year")
elif(y==0):
   print("enter a valid year")
elif(y%400==0)&(y%100!=0):
   print("The given year is leap year")
elif(y%4==0):
   print("The given year is leap year")
else:
   print("The given year is not leap year")
